{
  "name": "storyshot-workspace",
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm --filter storyshot build",
    "test": "pnpm --filter storyshot test",
    "format": "pnpm dlx prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "pnpm dlx prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "author": "geonhyeoklee",
  "keywords": [
    "storybook",
    "playwright"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.17.16"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  }
}
